		Employee e3=new Employee(1,"Samal",1000);
		Employee e4=new Employee(1,"Jhon",1000);
		List<Employee> listemp=new ArrayList<Employee>();
		listemp.add(e1);
		listemp.add(e2);
		listemp.add(e3);
		listemp.add(e4);
		
		List<Employee> l=list.stream().distinct().sorted().collect(Collectors.toList());
	}

}
