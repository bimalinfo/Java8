class ArrayListDuplicateRemoval {
	
 	public static <T> ArrayList<T> removeDuplicates(ArrayList<T> list) { 
		Set<T> set = new LinkedHashSet<>(); 
		set.addAll(list); 
		list.clear(); 
		list.addAll(set); 
		return list; 
	}
	
 	public static void main(String[] args) {
		

	}
	
} 